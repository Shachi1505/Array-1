class HelloWorld {

    // Print min element in array
    public static void main(String[] args) {
        int[] arr = { 2, 5, 9, 0, -3, -7, 21, 38, -11};
int min = arr[0];
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] < min) {
             arr[i] = min;
            }
                System.out.println(arr[i]);
            }
        }
    
}
